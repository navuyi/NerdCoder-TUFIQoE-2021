{
  "manifest_version": 2,
  "name": "NerdCoder",
  "version": "1.0",
  "description": "DEVELOPMENT build with simple auto-reloader\n[2021-08-16 18:08:18] waiting for changes...",
  "background": {
    "persistent": true,
    "scripts": [
      "assets/background-page-reloader-3c7a4d63.js",
      "assets/background-c101863b.js"
    ]
  },
  "permissions": [
    "storage",
    "webNavigation",
