{
  "manifest_version": 2,
  "name": "NerdCoder",
  "version": "1.0",
  "description": "Chrome extension for tracking YouTube activity and gathering nerd statistics",
  "background": {
    "persistent": false,
    "scripts": [
      "assets/background-c101863b.js"
    ]
  },
  "permissions": [
    "storage",
    "history",
    "webNavigation",
    "debugger",
    "activeTab",
    "tabs",
    "notifications",
    "https://*/",
    "background",
    "declarativeContent"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "init.js",
    "no_connection_screen.js",
    "mouse_tracker_script.js",
    "scenarios/*",
    "extension_pages/session_end/session_end.html",
    "yt_logout.js",
    "extension_pages/entry_screen/entry_screen.html",
    "background_controllers/AssessmentPanel.js",
    "scenario_training.json",
    "scenario_training_longer.json"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}