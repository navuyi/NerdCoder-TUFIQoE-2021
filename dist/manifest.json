{
  "manifest_version": 2,
  "name": "NerdCoder",
  "version": "1.0",
  "description": "DEVELOPMENT build with simple auto-reloader\n[2021-07-30 11:30:47] waiting for changes...",
  "background": {
    "persistent": true,
    "scripts": [
      "assets/background-page-reloader-81fc667f.js",
      "assets/background-f2be2f24.js"
    ]
  },
  "permissions": [
    "storage",
    "webNavigation",
    "debugger",
    "activeTab",
    "tabs",
    "notifications",
    "https://*/",
    "background",
    "declarativeContent"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "init.js",
    "no_connection_screen.js",
    "main_scenario.json",
    "training_scenario.json",
    "scenarios/*",
    "background_controllers/AssessmentController.js",
    "background_controllers/AssessmentPanel.js",
    "background_controllers/ChromeDebugger.js"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}