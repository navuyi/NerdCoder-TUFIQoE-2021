{
  "name": "NerdCoder",
  "description": "Chrome extension for tracking YouTube activity and gathering nerd statistics",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "debugger",
    "activeTab",
    "tabs",
    "notifications",
    "https://*/",
    "webNavigation",
    "background", "declarativeContent", "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["init.js", "no_connection_screen.js"]
}